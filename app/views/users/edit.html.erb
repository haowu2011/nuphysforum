<h1>Editing User </h1>

<form action="/users/<%= @user.id %>/update">
  <div>
    <label for="user_name">user_name</label><br />
    <input id="user_name" name="user_name" value="<%= @user.user_name  %>" type="text" />
  </div>
  <div>
    <label for="user_email">user_email</label><br />
    <input id="user_email" name="user_email" value="<%= @user.user_email  %>" type="text" />
  </div>
  <div>
    <label for="user_image">user_image</label><br />
    <input id="user_image" name="user_image" value="<%= @user.user_image  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


