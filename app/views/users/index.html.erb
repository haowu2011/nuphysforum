<h1>Users</h1>

<p><a href="/new_user">Add a New User</a></p>

<table>
  <thead>
    <tr>
      <th>User name</th>
      <th>User email</th>
      <th>User image</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
          <td><%= user.user_name %></td>
          <td><%= user.user_email %></td>
          <td> <img class="media-object" src="<%=user.user_image %>"></td>
          <td><a href="/users/<%= user.id %>">Show</a>
        </td>
      <td><a href="/users/<%= user.id %>/edit">Edit</a></td>
      <td><a href="/users/<%= user.id %>/destroy">Destroy</a></td>
    </tr>
  <% end %>
  </tbody>
</table>

